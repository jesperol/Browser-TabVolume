{
  "name": "Tab Volume v3",
  "description": "Control the volume of each tab individually.", 
  "version": "1.3.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "tabs",
    "tabCapture",
    "offscreen",
    "scripting",
    "storage"
  ], 
  "background": {
    "service_worker": "service_worker.js"
  },
  "host_permissions": [ "<all_urls>" ],
  "icons": {
    "16": "icons/dark/16.png",
    "48": "icons/dark/48.png",
    "128": "icons/dark/128.png"
  },
  "action": {
    "default_popup": "popup/index.html"
  }
}
